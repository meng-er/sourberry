  this.$router.push({ path: \"/doctor/hospitalization/list\" });\n      this.flag = \"check\";\n    },\n    add() {\n      this.$router.push({ path: \"/doctor/hospitalization/add\" });\n      this.flag = \"add\";\n    },\n  },\n  watch: {\n    \"$route.path\": {\n      immediate: true,\n      deep: true,\n      handler(newVal, oldVal) {\n        if (newVal === \"/doctor/hospitalization/add\") {\n          this.flag = \"add\";\n        } else {\n          this.flag = \"check\";\n        }\n      },\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n#hospitalization {\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n  .nav {\n    height: 42px;\n  }\n  .tab {\n    color: #333;\n    background: url(../../assets/img/title_bg.png) 0 -55px no-repeat;\n    display: inline-block;\n    width: 150px;\n    height: 42px;\n    line-height: 42px;\n    text-align: center;\n  }\n  .check_tab {\n    color: #fff;\n    background: url(../../assets/img/title_bg.png) 0 -8px no-repeat;\n    display: inline-block;\n    width: 150px;\n    height: 42px;\n    line-height: 42px;\n    text-align: center;\n  }\n  .content {\n    // width: 100%;\n    flex: 1;\n    background: rgba(255, 255, 255, 0.7);\n    padding: 30px;\n    box-sizing: border-box;\n  }\n}\n</style>\n"]}]}